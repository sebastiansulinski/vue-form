{
  "name": "@ssdcode/vue-form",
  "version": "1.1.12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastiansulinski/vue-form.git"
  },
  "homepage": "https://github.com/sebastiansulinski/vue-form#readme",
  "main": "./dist/vue-form.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-form ./src/wrapper.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@ssdcode/cms-partials": "^0.1.6",
    "@ssdcode/vue-event-bus": "^0.1.5",
    "@ssdcode/vue-focus-directive": "^0.1.6",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "foundation-sites": "^6.5.3",
    "node-sass": "^4.13.0",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
