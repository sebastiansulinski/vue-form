{
  "name": "@ssdcode/vue-form",
  "version": "1.10.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastiansulinski/vue-form.git"
  },
  "homepage": "https://github.com/sebastiansulinski/vue-form#readme",
  "main": "./dist/vue-form.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-form ./src/wrapper.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@ssdcode/cms-partials": "^1.3.1",
    "@ssdcode/vue-event-bus": "^0.1.7",
    "@ssdcode/vue-focus-directive": "^0.2.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "foundation-sites": "^6.6.3",
    "json-server": "^0.16.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "no-prototype-builtins": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "arrow-parens": [
      "as-needed",
      "ban-single-arg-parens"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
